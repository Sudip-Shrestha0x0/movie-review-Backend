      "required": true
    },
    "rating": {
      "type": "decimal",
      "required": true,
      "min": 1,
      "max": 10
    },
    "director": {
      "type": "string"
    },
    "releaseYear": {
      "type": "integer"
    },
    "genre": {
      "type": "string"
    },
    "imageUrl": {
      "type": "string"
    },
    "fullReview": {
      "type": "blocks",
      "required": true
    }
  }
}
